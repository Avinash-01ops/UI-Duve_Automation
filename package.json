{
    "name": "ui-duve_automation",
    "version": "1.0.0",
    "main": "index.js",
    "type": "commonjs",
    "scripts": {
        "test:regression": "playwright test --config=playwright.regression.config.js",
        "test:regression:headed": "playwright test --config=playwright.regression.config.js --headed",
        "dashboard": "playwright test src/tests/regression/dashboard-full.spec.js --config=playwright.regression.config.js",
        "dashboard:headed": "playwright test src/tests/regression/dashboard-full.spec.js --config=playwright.regression.config.js --headed",
        "rooms": "playwright test src/tests/regression/rooms.spec.js --config=playwright.regression.config.js",
        "rooms:headed": "playwright test src/tests/regression/rooms.spec.js --config=playwright.regression.config.js --headed",
        "reservations": "playwright test src/tests/regression/reservations.spec.js --config=playwright.regression.config.js",
        "reservations:headed": "playwright test src/tests/regression/reservations.spec.js --config=playwright.regression.config.js --headed",
        "create-room": "playwright test src/tests/regression/create-room.spec.js --config=playwright.regression.config.js",
        "create-room:headed": "playwright test src/tests/regression/create-room.spec.js --config=playwright.regression.config.js --headed",
        "show-report": "playwright show-report",
        "allure:generate": "allure generate src/test-results/allure-results --clean -o src/test-results/allure-report",
        "allure:open": "allure open src/test-results/allure-report",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test:smoke": "playwright test --config=playwright.smoke.config.js",
        "test:smoke:headed": "playwright test --config=playwright.smoke.config.js --headed",
        "test:mobile": "playwright test --config=playwright.mobile.config.js",
        "test:login:headed": "playwright test src/tests/smoke/login.spec.js --config=playwright.smoke.config.js --headed",
        "test:login": "playwright test src/tests/smoke/login.spec.js --config=playwright.smoke.config.js",
        "test:forgot-password": "playwright test src/tests/smoke/forgot-password.spec.js --config=playwright.smoke.config.js",
        "test:forgot-password:headed": "playwright test src/tests/smoke/forgot-password.spec.js --config=playwright.smoke.config.js --headed",
        "test:all": "npm run dashboard -- --workers=1 && npm run rooms -- --workers=1 && npm run reservations -- --workers=1 && npm run create-room -- --workers=1 && npm run test:smoke -- --workers=1 && npm run test:login -- --workers=1 && npm run test:forgot-password -- --workers=1 || echo ok"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@playwright/test": "^1.54.2",
        "allure-commandline": "^2.29.0",
        "allure-playwright": "^2.15.1",
        "eslint": "^8.57.0",
        "eslint-plugin-node": "^11.1.0"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "clipboardy": "^4.0.0",
        "csv-parse": "^5.5.6",
        "jquery": "^3.7.1",
        "moment": "^2.30.1",
        "mongoose": "^8.6.0",
        "newman": "^6.2.1",
        "request": "^2.88.2",
        "winston": "^3.13.0",
        "delay": "^6.0.0"
    }
}